#include <iostream>
#include "system.h"

coordinates::coordinates(short unsigned int x, short unsigned int y, 
			 short unsigned int z) {
  X = x;
  Y = y;
  Z = z;
}

short unsigned int coordinates::x() {
  return X;
} 

short unsigned int coordinates::y() {
  return Y;
}

short unsigned int coordinates::z() {
  return Z;
}
